map wb :set number!; set relativenumber!
map wh :set hidden!
map wr :set reverse!; push gg
map wn :set sortby name;  set info size
map ws :set sortby size;  set info size
map wt :set sortby time;  set info time
map wa :set sortby atime; set info atime
map wc :set sortby ctime; set info ctime
map we :set sortby ext

