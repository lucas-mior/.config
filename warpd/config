# Exit the currently active warpd session. (default: esc)
exit: esc

# Toggle drag mode (mnemonic (v)isual mode). (default: v)
# drag:

# Send the copy key and exit (useful in combination with v). (default: c)
# copy_and_exit:

# Increase the acceleration of the pointer while held. (default: a)
# accelerator:

# Decrease the speed of the pointer while held. (default: d)
# decelerator:

# A space separated list of mouse buttons (2 is middle click). (default: m , .)
buttons: m i o

# The mouse buttton used for dragging. (default: 1)
# drag_button:

# Oneshot mouse buttons (deactivate on click). (default: n - /)
# oneshot_buttons:

# Print the current mouse coordinates to stdout (useful for scripts). (default: p)
# print:

# Activate hint history mode while in normal mode. (default: ;)
# history:

# Activate hint mode while in normal mode (mnemonic: x marks the spot?). (default: x)
# hint:

# Activate two pass hint mode. (default: X)
# hint2:

# Activate (g)rid mode while in normal mode. (default: g)
# grid:

# Activate (s)creen selection while in normal mode. (default: s)
# screen:

# Move the cursor left in normal mode. (default: h)
left: j

# Move the cursor down in normal mode. (default: j)
down: k

# Move the cursor up in normal mode. (default: k)
up: l

# Move the cursor right in normal mode. (default: l)
right: ;

# Moves the cursor to the top of the screen in normal mode. (default: H)
# top:

# Moves the cursor to the middle of the screen in normal mode. (default: M)
# middle:

# Moves the cursor to the bottom of the screen in normal mode. (default: L)
# bottom:

# Moves the cursor to the leftmost corner of the screen in normal mode. (default: 0)
# start:

# Moves the cursor to the rightmost corner of the screen in normal mode. (default: $)
# end:

# Scroll down key. (default: e)
# scroll_down:

# Scroll up key. (default: r)
# scroll_up:

# The color of the pointer in normal mode (rgba hex value). (default: #FF4500)
# cursor_color:

# The height of the pointer in normal mode. (default: 7)
# cursor_size:

# The number of milliseconds before repeating a movement event. (default: 20)
# repeat_interval:

# Pointer speed in pixels/second. (default: 220)
speed: 200

# The maximum pointer speed. (default: 1600)
max_speed: 1200

# Pointer speed while decelerator is depressed. (default: 50)
# decelerator_speed:

# Pointer acceleration in pixels/second^2. (default: 700)
# acceleration:

# Pointer acceleration while the accelerator is depressed. (default: 2900)
# accelerator_acceleration:

# The length of time in milliseconds to wait for a second click after a oneshot key has been pressed. (default: 300)
# oneshot_timeout:

# History hint size as a percentage of screen height. (default: 2)
# hist_hint_size:

# The number of rows in the grid. (default: 2)
# grid_nr:

# The number of columns in the grid. (default: 2)
# grid_nc:

# Move to the last position in the history stack. (default: C-o)
# hist_back:

# Move to the next position in the history stack. (default: C-i)
# hist_forward:

# Move the grid up. (default: w)
# grid_up:

# Move the grid left. (default: a)
# grid_left:

# Move the grid down. (default: s)
# grid_down:

# Move the grid right. (default: d)
# grid_right:

# A sequence of comma delimited keybindings which are ordered bookwise with respect to grid position. (default: u i j k)
# grid_keys:

# Exit grid mode and return to normal mode. (default: c)
# grid_exit:

# The thickness of grid lines in pixels. (default: 4)
# grid_size:

# The thickness of the grid border in pixels. (default: 0)
# grid_border_size:

# The color of the grid. (default: #1c1c1e)
# grid_color:

# The color of the grid border. (default: #ffffff)
# grid_border_color:

# The background hint color. (default: #1c1c1e)
hint_bgcolor: #111111

# The foreground hint color. (default: #a1aba7)
hint_fgcolor: #eeeeee

# The character set from which hints are generated. The total number of hints is
# the square of the size of this string. It may be desirable to increase this
# for larger screens or trim it to increase gaps between hints.
# (default: abcdefghijklmnopqrstuvwxyz)
hint_chars: qwertyuiopasdfghjkl;cvbnm1234567890-

# The font name used by hints. Note: This is platform specific, in X it
# corresponds to a valid xft font name, on macos it corresponds to a postscript
# name. (default: Arial)
hint_font: Liberation

# Hint size (range: 1-1000) (default: 20)
hint_size: 22

# Border radius. (default: 3)
hint_border_radius: 1

# The exit key used for hint mode. (default: esc)
hint_exit: esc

# undo last selection step in one of the hint based modes. (default: backspace)
# hint_undo:

# undo all selection steps in one of the hint based modes. (default: C-u)
# hint_undo_all:

# The character set used for the second hint selection, should consist of at least hint_grid_size^2 characters. (default: hjkl;asdfgqwertyuiopzxcvb)
# hint2_chars:

# The size of hints in the secondary grid (range: 1-1000). (default: 20)
# hint2_size:

# The spacing between hints in the secondary grid. (range: 1-1000) (default: 1)
# hint2_gap_size:

# The size of the secondary grid. (default: 3)
# hint2_grid_size:

# The characters used for screen selection. (default: jkl;asdfg)
# screen_chars:

# Initial scroll speed in units/second (unit varies by platform). (default: 300)
# scroll_speed:

# Maximum scroll speed. (default: 9000)
# scroll_max_speed:

# Scroll acceleration in units/second^2. (default: 1600)
# scroll_acceleration:

# Scroll deceleration. (default: -3400)
# scroll_deceleration:

# Specifies an optional visual indicator to be displayed while normal mode is
# active, must be one of: topright, topleft, bottomright, bottomleft, none
# (default: none)
indicator: topleft

# The color of the visual indicator color.
indicator_color: #00ff00

# The size of the visual indicator in pixels. (default: 12)
indicator_size: 40
